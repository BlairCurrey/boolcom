{
  "name": "pperm",
  "version": "1.0.0",
  "description": "cli utility to generate permutations and combinations",
  "main": "dist/index.js",
  "bin": {
    "pperm": "dist/cli.js"
  },
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "start": "node dist/index.js"
  },
  "author": "Blair Currey",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.12",
    "ava": "^3.15.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
