{
  "name": "boolcom",
  "version": "1.0.0",
  "description": "cli utility to generate permutations and combinations",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlairCurrey/boolcom"
  },
  "bin": {
    "boolcom": "lib/cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "./node_modules/.bin/eslint --ext .js,.ts, src",
    "start": "node lib/index.js"
  },
  "author": "Blair Currey",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "ava": "^3.15.0",
    "eslint": "^8.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
